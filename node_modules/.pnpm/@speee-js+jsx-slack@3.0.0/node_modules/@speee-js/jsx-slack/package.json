{
  "name": "@speee-js/jsx-slack",
  "version": "3.0.0",
  "description": "An alias package for jsx-slack",
  "author": {
    "name": "Yuki Hattori",
    "url": "https://github.com/yhatt"
  },
  "license": "MIT",
  "homepage": "https://github.com/yhatt/jsx-slack#readme",
  "bugs": {
